  class Messages(dd.Table):

      parameters = dict(
         non_assigned=models.BooleanField(
            _("show only non assigned"),
            default=False))
 

    @classmethod
    def get_request_queryset(self, ar):
        qs = super(Messages, self).get_request_queryset(ar)
        pv = ar.param_values

        if pv.non_assigned:
            qs = qs.filter(pointers__isnull=False)

        return qs           

class UnassignedMessages(Messages):

    @classmethod
    def param_defaults(self, ar, **kw):
        kw = super(Messages, self).param_defaults(ar, **kw)
        kw.update(non_assigned=True)
        return kw


